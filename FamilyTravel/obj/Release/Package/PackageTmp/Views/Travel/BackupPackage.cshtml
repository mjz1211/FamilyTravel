    <script type="text/javascript">
        function updateLocation(mode, ptype0){
            if(mode == 'AUTO'){
                if (navigator.geolocation) {
                    navigator.geolocation.getCurrentPosition(success);
                } else {
                    alert("Geo Location is not supported on your current browser");
                }
                $('#address').hide();
                $('#query').hide();

            }

            else{
                $('#address').show();
                $('#query').show();
            }
        }


        var geocoder = new google.maps.Geocoder();

        function geocodeAddress(geocoder) {
            var address = document.getElementById('address').value;
            geocoder.geocode({ 'address': address }, function (results, status) {
                if (status === 'OK') {
                    //console.log(results[0].geometry.location.lat());
                    //console.log(results[0].geometry.location.lng());
                    lat = results[0].geometry.location.lat();
                    lng = results[0].geometry.location.lng();
                    console.log(results);
                    search(0,0); // 0,0 use global variable lat, lng

                } else {
                    alert('Geocode was not successful for the following reason: ' + status);
                }
            });
        }

        var lat,lng;
        function success(position) {
            lat = position.coords.latitude;
            lng = position.coords.longitude;
            @{
                if (ViewBag.Lat != null && ViewBag.Lng != null) {
                    <text>
            lat = @ViewBag.Lat;
            lng = @ViewBag.Lng;
            </text>
                }
            }
            search(lat, lng);
        }

        function searchByMagic(ptype0){
            //console.log(ptype0 + '...' )

            var cLocation = new google.maps.LatLng(lat, lng);
            var request = {
                location: cLocation,
                radius: '10000',
                type: ptype0
            };

            service = new google.maps.places.PlacesService(document.createElement('div'));
            service.nearbySearch(request, callback2);
        }

        var magicMax = 0;
        function callback2(results, status){
            if (status == google.maps.places.PlacesServiceStatus.OK) {
                magicMax = 0;
                for (var i = 0; i < results.length ; i++) {

                    place = results[i];
                    if( place.place_id ==  $('#Place_Id_' + currentIndex ).val() )
                        continue;


                    if(place.rating != undefined){

                        var tempMax = parseFloat(place.rating);

                        if( tempMax == 5 )
                            continue;
                        if( tempMax > magicMax ){
                            magicMax = tempMax;
                        }
                    }

                }

                for (var i = 0; i < results.length ; i++) {
                    place = results[i];
                    var tempMax = parseFloat(place.rating);
                    if( tempMax == magicMax ){
                        break;
                    }
                }

                if(place.photos == undefined){
                    imgUrl = place.icon;
                }
                else{
